{
  "compilerOptions": {
    "emitBOM": true,
    "lib": [ "es6" ],
    "module": "commonjs",
    "noImplicitThis": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [ "./*" ]
    },
  },
  "exclude": [
    "./node_modules/*",
    "./node_modules/socket.io-client"
  ]
}
